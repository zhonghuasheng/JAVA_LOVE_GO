<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
                            http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <bean id="application" class="com.zhonghuasheng.spring4.autowiringusingxml.autowirebyname.Application" autowire="byName" />
    <bean id="applicationUser" class="com.zhonghuasheng.spring4.autowiringusingxml.autowirebyname.ApplicationUser">
        <property name="name" value="superUser"/>
    </bean>
</beans>
